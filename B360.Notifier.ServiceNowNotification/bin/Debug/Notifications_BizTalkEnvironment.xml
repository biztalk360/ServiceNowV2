<?xml version="1.0"?>
<MonitorGroupData
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <name>BizTalkEnvironment</name>
  <monitorStatus>Critical</monitorStatus>
  <isAutoCorrected>false</isAutoCorrected>
  <monitorGroups>
    <MonitorGroup>
      <name>BizTalk Environment</name>
      <monitorStatus>Critical</monitorStatus>
      <isAutoCorrected>false</isAutoCorrected>
      <monitors>
        <Monitor>
          <name>HostInstances</name>
          <serializedConfig />
